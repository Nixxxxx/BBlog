<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
     "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="true"/>
	
	<package name="BBlog" namespace="/"  extends="struts-default">
	
		<action name="user" class="com.action.UserAction">
			<result name="success">/main.jsp</result>
			<result name="error">/login.jsp</result>
		</action>
		
		<action name="main" class="com.action.MainAction">
			<result name="success">/main.jsp</result>
		</action>
		
		<action name="blogType" class="com.action.BlogTypeAction">
			<result name="success">/main.jsp</result>
		</action>
		
		<action name="blog" class="com.action.BlogAction">
			<result name="success">/main.jsp</result>
		</action>
	</package>
</struts>
